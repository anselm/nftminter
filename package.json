{
  "name": "NFTMint",
  "version": "1.0.0",
  "description": "Generic Mint and Store",
  "main": "index.js",
  "private": false,
  "scripts": {
    "compile": "npx hardhat compile",
    "coverage": "npx hardhat coverage",
    "size": "yarn compile && npx hardhat size-contracts",
    "deploy:mint": "npx hardhat run scripts/deploy-mint.ts",
    "deploy:store": "npx hardhat run scripts/deploy-store.ts",
    "flatten:pass": "npx hardhat flatten contracts/NFTMint.sol > flatten/NFTMint.sol",
    "flatten:store": "npx hardhat flatten contracts/NFTStore.sol > flatten/NFTStore.sol",
    "clean": "npx hardhat clean",
    "sign:pass": "npx hardhat run scripts/sign-message.ts"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.1",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eth-typed-data": "^0.1.0-beta.0",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-util": "^7.1.3",
    "ethers": "^5.5.2",
    "hardhat": "^2.8.0",
    "hardhat-contract-sizer": "^2.1.1",
    "hardhat-deploy": "^0.9.14",
    "hardhat-gas-reporter": "^1.0.6",
    "hardhat-tracer": "^1.0.0-alpha.6",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "typechain": "^6.0.5",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.1.9",
    "@openzeppelin/contracts": "^4.4.1",
    "eth-typed-data": "^0.1.0-beta.0",
    "ethereumjs-util": "^7.1.3",
    "web3": "^1.6.1"
  }
}
